</head>

<body>
<%- include('../includes/navigation.ejs') %>
<main>
    <% if (orders.length ) { %>
        <ul>
            <% for (let { id, products } of orders) { %>
                <li>
                    <h1><%= id %></h1>
                    <ul>
                        <% for (let { title, orderItem } of products) { %>
                            <li><%= title %>(<%= orderItem.quantity %>)</li>
                        <% } %>
                    </ul>
                </li>
            <% } %>
        </ul>
    <% } else { %>
        <h1>Nothing there!</h1>
    <% } %>
</main>
<%- include('../includes/end.ejs') %>
